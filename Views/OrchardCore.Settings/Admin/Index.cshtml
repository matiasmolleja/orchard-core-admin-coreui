@model AdminIndexViewModel
    <zone name="PageTitle">
        <h1 class="page-title">@RenderTitleSegments(T["Settings"])</h1>
    </zone>



    <div class="container-fluid">
        <div class="card">
            <div class="card-header">
                @T["Settings"]
            </div>
            <div class="card-body">

                <form asp-action="Index" asp-route-returnUrl="@Context.Request.Query["ReturnUrl"]" method="post" enctype="multipart/form-data">
                    <input asp-for="GroupId" type="hidden" />
                    @Html.ValidationSummary()
                    @await DisplayAsync(Model.Shape)
                </form>
            </div>
        </div>
    </div>
